<config>
    <modules>
        <Lkwebtools_Facebookpage>
            <version>1.0.0</version>
        </Lkwebtools_Facebookpage>
    </modules>
    <frontend>
        <routers>
            <facebookpage>
                <use>standard</use>
                <args>
                    <module>Lkwebtools_Facebookpage</module>
                    <frontName>facebookpage</frontName>
                </args>
            </facebookpage>
        </routers>
        <layout>
            <updates>
                <facebookpage>
                    <file>facebookpage.xml</file>
                </facebookpage>
            </updates>
        </layout>
    </frontend>
      <global>
        <models>
            <facebookpage>
                <class>Lkwebtools_Facebookpage_Model</class>
            </facebookpage>
        </models>
        <blocks>
            <facebookpage>
                <class>Lkwebtools_Facebookpage_Block</class>
            </facebookpage>
        </blocks>
        <helpers>
            <facebookpage>
                <class>Lkwebtools_Facebookpage_Helper</class>
            </facebookpage>
        </helpers>
    </global>
</config>